		_FwdIt _Dest, _Alloc& _Al)
	{	// copy [_First, _Last) to raw _Dest, using _Al
	return (_STD _Uninit_copy(_CHECKED_BASE(_First),
		_CHECKED_BASE(_Last), _Dest, _Al,
		_STD _Ptr_cat(_First, _Dest), _STD _Range_checked_iterator_tag()));
	}

