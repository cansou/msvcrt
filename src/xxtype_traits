template<class _Ret _C_CLASS_ARG0>
	struct _Is_funptr<_Ret (*)(_ARG0_ARG1)>
	: true_type
	{	// base class for function pointer predicates
	};

template<class _Ret _C_CLASS_ARG0>
	struct _Is_funptr<_Ret (*)(_ARG0_ARG1 ...)>
	: true_type
	{	// base class for function pointer predicates
	};

